    "import turtle\n",
    "t=turtle.Turtle()\n",
    "s=turtle.Screen()\n",
    "s.bgcolor(\"black\")\n",
    "t.speed(0)\n",
    "\n",
    "col=[\"yellow\",\"red\",\"pink\",\"cyan\",\"green\",\"blue\"]\n",
    "\n",
    "for i in range(120):\n",
    "    t.pencolor(col[i%6])\n",
    "    t.circle(190-i/2,90)\n",
    "    t.lt(90)\n",
    "    t.circle(190-i/3,90)\n",
    "    t.lt(60)\n",
    "\n",
    "s.exitonclick()"
   ]
